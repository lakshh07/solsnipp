{
  "name": "web3-solsnipp",
  "displayName": "SolSnipp",
  "description": "Snippets for solidity",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Snippets"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [
    "onLanguage:solidity",
    "onCommand:web3-solsnipp.helloWorld"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "web3-solsnipp.helloWorld",
        "title": "Hello World"
      }
    ],
    "snippets": [
      {
        "language": "html",
        "path": "./src/snippets/generated.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
